
# Configurações de Desenvolvimento
NODE_ENV=development
PORT=5000

# Configurações de Sessão
SESSION_SECRET=dev-secret-key-not-for-production

# IMPORTANTE: Configure sua URL do PostgreSQL aqui
# Copie a mesma DATABASE_URL que você usa em produção
DATABASE_URL=postgresql://seu_usuario:sua_senha@seu_host:5432/seu_banco

# Para SSL em produção
NODE_TLS_REJECT_UNAUTHORIZED=0
